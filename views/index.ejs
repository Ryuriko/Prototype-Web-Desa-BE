    <div class="col-10">
        <p class="h1 text-center my-5">Desa Carita</p>
        <div id="carouselExampleAutoplaying" class="carousel slide" data-bs-ride="carousel">
            <div class="carousel-inner">
              <div class="carousel-item active">
                <img src="./img/carousel1.jpg" class="d-block img-thumbnail">
                <h5 class="text-center bg-body-tertiary">Carita</h5>
              </div>
              <div class="carousel-item">
                <img src="./img/carousel2.jpg" class="d-block img-thumbnail">
